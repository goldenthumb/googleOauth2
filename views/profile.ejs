<!doctype html>
<html lang="ko">
<head>
  <title>Node Authentication</title>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <style>
    body { padding-top: 80px; }
  </style>
</head>
<body>
<div class="container">
  <div class="page-header text-center">
    <h1>Profile Page</h1>
    <a href="/logout" class="btn btn-default btn-sm">Logout</a>
  </div>
  <div class="row">
    <!-- GOOGLE INFORMATION -->
    <div class="col-sm-12">
      <div class="profile-example">
        <h3><span class="fa fa-google-plus"></span>Google</h3>
        <% if (user.googleToken) { %>
        <p>
          <strong>img</strong>: <img src ="<%=user.googlePhotoPath %>" /><br>
          <strong>id</strong>: <%=user.googleId %><br>
          <strong>token</strong>: <%=user.googleToken %><br>
          <strong>email</strong>: <%=user.googleEmail %><br>
          <strong>name</strong>: <%=user.googleName %>
        </p>
        <a href="/unlink/google" class="btn btn-danger">Unlink</a>
        <% } else { %>
        <a href="/connect/google" class="btn btn-danger">Connect Google</a>
        <% } %>
      </div>
    </div>
  </div>
  <div class="text-center out-links"><a href="#">Â© goldenthumb</a></div>
</div>
</body>
</html>